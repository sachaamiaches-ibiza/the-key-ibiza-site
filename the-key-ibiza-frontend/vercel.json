{
  "rewrites": [
    {
      "source": "/villa-:slug*",
      "has": [
        {
          "type": "header",
          "key": "user-agent",
          "value": ".*(facebookexternalhit|Facebot|WhatsApp|Twitterbot|LinkedInBot|Pinterest|Slackbot|TelegramBot|Discordbot).*"
        }
      ],
      "destination": "https://the-key-ibiza-backend.vercel.app/og/villa/:slug*"
    },
    {
      "source": "/yacht-:slug*",
      "has": [
        {
          "type": "header",
          "key": "user-agent",
          "value": ".*(facebookexternalhit|Facebot|WhatsApp|Twitterbot|LinkedInBot|Pinterest|Slackbot|TelegramBot|Discordbot).*"
        }
      ],
      "destination": "https://the-key-ibiza-backend.vercel.app/og/yacht/:slug*"
    },
    {
      "source": "/((?!sitemap\\.xml|robots\\.txt|assets/).*)",
      "destination": "/index.html"
    }
  ]
}
